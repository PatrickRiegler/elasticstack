    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "elasticstack-pipeline",
        "labels": {
          "sdbig": "elasticstack",
          "sdbi": "elasticstack"
        },
        "annotations": {
          "pipeline.alpha.openshift.io/uses": "[{\"name\": \"jenkins\", \"kind\": \"DeploymentConfig\"}]"
        }
      },
      "spec": {
        "source": {
          "type": "Git",
          "git": {
            "uri": "ssh://git@stash.six-group.net:22/sdbi/elasticstack.git",
            "ref": "develop"
          },
          "sourceSecret": {
            "name": "bitbucket-secret"
          }
        },
        "strategy": {
          "type": "JenkinsPipeline",
          "jenkinsPipelineStrategy": {
            "jenkinsfilePath": "elasticsearch/Jenkinsfile"
          }
        },
        "triggers": [
            {
                "type": "ImageChange",
                "imageChange": {
                    "from": {
                        "kind": "ImageStreamTag",
                        "name": "oracle-java-server-jre-8:latest"
                    }
                }
            }
        ]
      }
    },
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "elasticsearch-build",
        "labels": {
          "sdbig": "elasticstack",
          "sdbi": "elasticsearch"
        }
      },
      "spec": {
        "runPolicy": "Serial",
        "source": {
          "type": "Git",
          "git": {
            "uri": "ssh://git@stash.six-group.net:22/sdbi/elasticstack.git"
          },
          "sourceSecret": {
            "name": "bitbucket-secret"
          }
        },
        "strategy": {
          "type": "Docker",
          "dockerStrategy": {
            "from": {
              "kind": "ImageStreamTag",
              "name": "	oracle-java-server-jre-8:latest"
            },
            "forcePull": true,
            "dockerfilePath": "elasticsearch/Dockerfile"
          }
        },
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "elasticsearch-build:tmp"
          }
        }
      }
    },
